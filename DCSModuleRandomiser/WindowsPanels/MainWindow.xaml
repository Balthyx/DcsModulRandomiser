<Window x:Class="DCSModuleRandomiser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DCSModuleRandomiser"
        mc:Ignorable="d"
        Title="DCS Module Randomiser" Height="119" Width="344" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="OutputField" Text="F-16C | Persian Gulf Modern" TextWrapping="Wrap" Margin="100,5,5,5" IsEnabled="False"/>
        <Label>
            <Button x:Name="BtnReroll" Content="Reroll" Height="25"/>
        </Label>
    </Grid>
</Window>
