        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DCSModuleRandomiser"
        mc:Ignorable="d"
        Title="DCS Module Randomiser" Height="75" Width="340" ResizeMode="NoResize">
